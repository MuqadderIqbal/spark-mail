@version("1.0.0")
@namespace("mailrecord")
protocol MailRecordProtocol {
  record MailRecord {
    string uuid;
    string from;
    union{array<string>,null} to = null;
    union{array<string>,null} cc = null;
    union{array<string>,null} bcc = null;
    long dateUtcEpoch;
    string subject;
    union{map<string>,null} mailFields = null;
    string body;
  }
} 